{
  "name": "@gallolabs/remote-ee",
  "version": "1.0.0",
  "description": "Remote events emitter",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -Rf dist && tsc",
    "test": "npm run build && npx c8 --all --src 'src' --exclude '*/*test*' --report-dir=/tmp/nyc mocha 'dist/*test*.js'",
    "deploy": "npm run test && rm dist/*test* && npm login && trap 'npm logout' EXIT && release-it --no-git.requireCleanWorkingDir"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/uri-templates": "^0.1.34",
    "c8": "^10.1.2",
    "mocha": "^10.2.0",
    "nock": "^13.5.5",
    "release-it": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "."
  ],
  "dependencies": {
    "@gallolabs/nanodate": "^0.2.3",
    "got": "^14.4.3",
    "lodash-es": "^4.17.21",
    "matcher": "^5.0.0",
    "uri-templates": "^0.2.0"
  }
}
